{
  "name" : "comai/arduino",
  "description": "Public api for arduino components",
  "require" : {
    "slim/slim" : "^4",
    "slim/psr7" : "^0.4",
    "php-di/php-di" : "^6",
    "ext-json" : "*",
    "ext-pdo" : "*",
    "ext-dom" : "*",
    "ext-simplexml" : "*",
    "ext-mbstring" : "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8",
    "phpstan/phpstan": "^0.11",
    "squizlabs/php_codesniffer": "^3"
  },
  "autoload": {
    "psr-4": {
      "ComAI\\ArduComponents\\" : "src/",
      "ComAI\\ArduComponents\\Tests\\" : "tests/"
    }
  }
}